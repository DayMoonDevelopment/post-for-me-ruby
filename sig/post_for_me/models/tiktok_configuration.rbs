module PostForMe
  module Models
    type tiktok_configuration =
      {
        allow_comment: bool?,
        allow_duet: bool?,
        allow_stitch: bool?,
        caption: top?,
        disclose_branded_content: bool?,
        disclose_your_brand: bool?,
        is_ai_generated: bool?,
        is_draft: bool?,
        media: ::Array[String]?,
        privacy_status: String?,
        title: String?
