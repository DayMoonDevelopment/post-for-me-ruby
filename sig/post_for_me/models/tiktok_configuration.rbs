      attr_accessor is_draft: bool?

      attr_accessor media: ::Array[String]?

      attr_accessor privacy_status: String?

      attr_accessor title: String?

      def initialize: (
        ?allow_comment: bool?,
        ?allow_duet: bool?,
        ?allow_stitch: bool?,
        ?caption: top?,
        ?disclose_branded_content: bool?,
        ?disclose_your_brand: bool?,
        ?is_ai_generated: bool?,
        ?is_draft: bool?,
        ?media: ::Array[String]?,
        ?privacy_status: String?,
        ?title: String?
      ) -> void

      def to_hash: -> {
        allow_comment: bool?,
        allow_duet: bool?,
        allow_stitch: bool?,
        caption: top?,
        disclose_branded_content: bool?,
        disclose_your_brand: bool?,
        is_ai_generated: bool?,
        is_draft: bool?,
        media: ::Array[String]?,
        privacy_status: String?,
        title: String?
      }
